      function parseText(text, size){
          var temp_text = text
          if(temp_text.length > size){
              temp_text = temp_text.slice(0,size-3);
              temp_text = temp_text.concat('...');
          }
          temp_text = temp_text.replace('amp;', '');
          return temp_text;
      }
      google.setOnLoadCallback(initialize);
